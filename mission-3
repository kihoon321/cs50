//2주차 팀미션 3번문제
#include <stdio.h>
#include <cs50.h>

int main (void)
{
    string 월요일 = "청국장";
    string 화요일 = "비빔밥";
    string 수요일 = "된장찌개";
    string 목요일 = "칼국수";
    string 금요일 = "냉면";
    string 토요일 = "소불고기";
    string 일요일 = "오삼불고기";

    string 요일 = get_string("요일을 입력하세요: ");
    if (요일 == 월요일)
    {
        printf("월요일: %s\n", 월요일);
    }
   else if (요일 == 화요일) 
   {
       printf("화요일: %s\n", 화요일);
   }
}
